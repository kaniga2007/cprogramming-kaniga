#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char name[50];
    int m1,m2,m3,m4,m5,total;
    float avg;
    scanf("%s",name);
    printf("name of the student;%s\n",name);
    scanf("%d",&m1);
    scanf("%d",&m2);
    scanf("%d",&m3);
    scanf("%d",&m4);
    scanf("%d",&m5);
    total=m1+m2+m3+m4+m5;
    if("total mark %.1f\n",total);
    avg=total/50;
    printf("average mark:%.1f\n"average);
    if(avg==100)
    {
        printf("grade mark:S");
    }
    else if(avg>90)
    {
        printf("grade mark:A");
    }
    else if(avg>80)
    {
        printf("grade mark:B");
    }
    else if(avg>70)
    {
        print("grade mark:C");
    }
    else if(avg>60)
    {
      printf("grade mark:D");  
    }
    else if(avg>50)
    {
        printf("grade mark:E");
    }
    else
    {
        printf("grade mark:fail");
    }
    return 0;
}
