#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int a,n;
    scanf("%d",&n);
    if((n>=1)&&(n<=9))
    {
        for(a=1;a<=5;a++)
        {
            int f = a * n;
        printf("%d x %d = %d\n",n,a,f);
    }
       }
else{
    printf("Invalid Input\n");
}   
    return 0;
}
