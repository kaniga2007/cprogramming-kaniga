#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int no_of_Liters,distance_converd;
    scanf("%d",&no_of_Liters);
    scanf("%d",&distance_converd);
    if((no_of_Liters)&&(distance_converd))
    {
       float Lit_km=((float)no_of_Liters/distance_converd)*100;
       printf("Liters/100KM\n%.2f\n",Lit_km);
       float miles = distance_converd * 0.6214;
       float gallons = no_of_Liters * 0.2642;
       float mil_gal = miles / gallons;
       printf("Miles/gallons\n%.2f",mil_gal);
    }
    else{
        printf("Invalid Input\n");
        }
    return 0;
}
